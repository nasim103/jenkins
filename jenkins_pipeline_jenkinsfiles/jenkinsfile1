pipeline {
    agent any
    environment {
        RELEASE='20.04'
    }

    stages {
        stage ('Build') {
            agent any
            environmnt {
                LOG_LEVEL='INFO'
    
            }

            steps {
                echo "Building Release $RELEASE with log level $LOG_LEVEL....."

            }
        }

        stage ('Test') {
            steps {
                echo "I can release $RELEASE but not log level $LOG_LEVEL"
            }
        }
    }
}